{
  "name": "Chrome sidebar",
  "version": "0.1.0",
  "description": "Chrome sidebar extension to embed an iframe.",
  "web_accessible_resources": ["entry.js"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Chrome sidebar"
  },
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ]
}
